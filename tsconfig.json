{
  "compilerOptions": {
    "target": "ESNext",
    "module": "NodeNext",
    "moduleResolution": "NodeNext",
    "lib": [
      "es2019",
      "dom",
      "webworker"
    ],
    "allowJs": true,
    "checkJs": true,
    "skipLibCheck": true,
    "noEmit": true,
    "strict": true,
    "esModuleInterop": true
  },
  "include": [
    "cases/**/*.js"
  ]
}
